[
  {
    "type": "console.page/route",
    "properties": {
      "exact": true,
      "path": "/k8s/ns/:ns/example",
      "component": { "$codeRef": "ExamplePage" }
    }
  },
  {
    "type": "console.page/route",
    "properties": {
      "exact": true,
      "path": "/k8s/all-namespaces/example",
      "component": { "$codeRef": "ExamplePage" }
    }
  },
  {
    "type": "console.navigation/section",
    "properties": {
      "id": "connectivity-link-section",
      "name": "%plugin__console-plugin-template~Connectivity Link%",
      "perspective": "dev"
    }
  },
  {
    "type": "console.navigation/href",
    "properties": {
      "id": "example",
      "name": "%plugin__console-plugin-template~Dashboard%",
      "href": "/k8s/ns/default/example",  // Use 'default' or another existing namespace
      "perspective": "dev",
      "section": "connectivity-link-section"
    }
  }
]
